<configuration>
    <system.web>
      <compilation targetFramework="4.6" />
      <httpRuntime targetFramework="4.6" />
    </system.web>
  <system.webServer>
    <httpErrors errorMode="Custom" defaultResponseMode="File">
      <remove statusCode="404"/>
      <error statusCode="404" path="index.html"/>
    </httpErrors>
  </system.webServer>
</configuration>
